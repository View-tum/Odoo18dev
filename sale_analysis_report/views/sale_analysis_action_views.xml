<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="action_sale_analysis_report" model="ir.actions.act_window">
        <field name="name">Sales Analysis Report</field>
        <field name="res_model">sale.analysis.report</field>
        <field name="view_mode">form</field>
        <field name="target">new</field>
    </record>

    <record id="action_sale_analysis_saleman_xlsx" model="ir.actions.report">
        <field name="name">Sale Analysis Saleman Xlsx</field>
        <field name="model">sale.analysis.report</field>
        <field name="report_type">xlsx</field>
        <field name="report_name">sale_analysis_report.sale_analysis_saleman_xlsx</field>
        <field name="report_file">sale_analysis_report.sale_analysis_saleman_xlsx</field>
        <field name="binding_model_id" eval="False"/>
        <field name="print_report_name">object._get_report_filename()</field>
    </record>

