<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="stock_location_repack" model="stock.location">
        <field name="name">Repack</field>
        <field name="usage">internal</field>
        <field name="location_id" ref="stock.stock_location_locations_virtual"/>
        <field name="company_id" eval="False"/>
        <field name="active">True</field>
    </record>

    <record id="seq_product_lot_transfer" model="ir.sequence">
        <field name="name">Product lot transfer sequence</field>
        <field name="code">product.lot.transfer</field>
        <field name="prefix">LTR/%(year)s/</field>
        <field eval="1" name="number_next" />
        <field eval="1" name="number_increment" />
        <field eval="True" name="use_date_range" />
        <field eval="False" name="company_id" />
        <field name="padding">6</field>
    </record>

</odoo>
