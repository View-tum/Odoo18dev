<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="seq_delivery_send" model="ir.sequence">
        <field name="name">DPT Delivery Send</field>
        <field name="code">delivery.send</field>
        <field name="prefix">DPT-DS/%(year)s/</field>
        <field name="padding">4</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="seq_transport_pickup" model="ir.sequence">
        <field name="name">DPT Transport Pickup</field>
        <field name="code">transport.pickup</field>
        <field name="prefix">DPT-TP/%(year)s/</field>
        <field name="padding">4</field>
        <field name="company_id" eval="False"/>
    </record>
    
    <record id="seq_customer_percel_list" model="ir.sequence">
        <field name="name">DPT Customer Percel List</field>
        <field name="code">customer.parcel.list</field>
        <field name="prefix">DPT-CPL/%(year)s/</field>
        <field name="padding">4</field>
        <field name="company_id" eval="False"/>
    </record>
</odoo>
