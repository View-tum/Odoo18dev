<odoo>
    <record id="view_stock_move_line_detailed_operation_tree_inherit_mrp_parallel" model="ir.ui.view">
        <field name="name">stock.move.line.detailed.operation.tree.inherit.mrp.parallel</field>
        <field name="model">stock.move.line</field>
        <field name="inherit_id" ref="stock.view_stock_move_line_detailed_operation_tree"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='quantity']" position="before">
                <field name="qty_on_hand_show" string="On Hand" readonly="1" decoration-bf="1" optional="show"/>
            </xpath>
        </field>
    </record>
</odoo>
