<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_account_fixed_asset_report_form" model="ir.ui.view">
        <field name="name">account.fixed.asset.report.form</field>
        <field name="model">account.fixed.asset.report</field>
        <field name="arch" type="xml">
            <form string="Fixed Asset Report">
                <group col="2">
                    <field name="asset_model_id"/>
                    <field name="asset_location_id"/>
                    <field name="asset_status"/>
                </group>
                <footer>
                    <button string="Export Excel" name="action_excel" type="object" class="btn-primary"/>
                    <button string="Cancel" special="cancel" class="btn-secondary"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>
