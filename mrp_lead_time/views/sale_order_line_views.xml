<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_order_form_inherit_mfg_lead_time" model="ir.ui.view">
        <field name="name">sale.order.form.inherit.mfg.lead.time</field>
        <field name="model">sale.order</field>
        <field name="inherit_id" ref="sale.view_order_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='order_line']/list//field[@name='qty_delivered']" position="after">
                <field name="mfg_lead_time"/>
            </xpath>
        </field>
    </record>
</odoo>