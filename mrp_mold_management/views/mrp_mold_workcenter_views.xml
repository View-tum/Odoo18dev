<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="mrp_mold_workcenter_form" model="ir.ui.view">
        <field name="name">mrp.workcenter.mold.form</field>
        <field name="model">mrp.workcenter</field>
        <field name="inherit_id" ref="mrp.mrp_workcenter_view"/>
        <field name="arch" type="xml">
            <xpath expr="//group[@name='capacity']" position="after">
                <group string="Mold">
                    <field name="is_mold"/>
                    <field name="mold_cost_hour"/>
                </group>
            </xpath>
        </field>
    </record>
</odoo>
