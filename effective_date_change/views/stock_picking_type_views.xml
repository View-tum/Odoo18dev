<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_picking_type_form_inherit_effective_date" model="ir.ui.view">
        <field name="name">stock.picking.type.form.inherit.effective.date</field>
        <field name="model">stock.picking.type</field>
        <field name="inherit_id" ref="stock.view_picking_type_form" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='auto_show_reception_report']" position="after">
                <field name="allow_edit_effective_date" />
                <field name="default_effective_date_offset" />
            </xpath>
        </field>
    </record>
</odoo>