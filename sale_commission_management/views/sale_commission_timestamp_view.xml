<odoo>
    <record id="view_sale_commission_timestamp_tree" model="ir.ui.view">
        <field name="name">sale.commission.timestamp.tree</field>
        <field name="model">sale.commission.timestamp</field>
        <field name="arch" type="xml">
            <list string="Commission History" create="0" edit="0" delete="1">
                <field name="log_date"/>
                <field name="salesperson_id"/>
                <field name="sale_order_name"/>
                <field name="invoice_names"/>
                <field name="commission_amount" sum="Total Commission"/>
            </list>
        </field>
    </record>

    <record id="view_sale_commission_timestamp_search" model="ir.ui.view">
        <field name="name">sale.commission.timestamp.search</field>
        <field name="model">sale.commission.timestamp</field>
        <field name="arch" type="xml">
            <search string="Search History">
                <field name="salesperson_id"/>
                <field name="sale_order_name"/>
                <field name="invoice_names"/>
                <group expand="1" string="Group By">
                    <filter string="Salesperson" name="group_salesperson" context="{'group_by':'salesperson_id'}"/>
                    <filter string="Date" name="group_date" context="{'group_by':'log_date:day'}"/>
                </group>
            </search>
        </field>
    </record>

    <record id="action_sale_commission_timestamp" model="ir.actions.act_window">
        <field name="name">Commission History</field>
        <field name="res_model">sale.commission.timestamp</field>
        <field name="view_mode">list</field>
    </record>
</odoo>