<odoo>
  <record id="view_sale_commission_rate_list" model="ir.ui.view">
    <field name="name">sale.commission.rate.list</field>
    <field name="model">sale.commission.rate</field>
    <field name="arch" type="xml">
      <list>
        <field name="name"/>
        <field name="value"/>
        <field name="description"/>
      </list>
    </field>
  </record>

  <record id="view_sale_commission_rate_form" model="ir.ui.view">
    <field name="name">sale.commission.rate.form</field>
    <field name="model">sale.commission.rate</field>
    <field name="arch" type="xml">
      <form>
        <sheet>
          <group>
            <field name="value"/>
            <field name="description"/>
          </group>
        </sheet>
      </form>
    </field>
  </record>

  <record id="action_sale_commission_rate" model="ir.actions.act_window">
    <field name="name">Commission Rates</field>
    <field name="res_model">sale.commission.rate</field>
    <field name="view_mode">list,form</field>
  </record>
</odoo>